<%= back_link_to admin_settings_path %>
<h2><%= t("admin.settings.index.social.title") %></h2>

<% @settings.each do |setting_group| %>
  <h3><%= t("admin.settings.index.social.#{setting_group.first.key.split(".").second}") %></h3>
  <%= render "mixed_settings_table", settings: setting_group %>
<% end %>

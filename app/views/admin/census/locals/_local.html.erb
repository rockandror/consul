<tr id="<%= dom_id(local) %>" class="census_local">
  <td>
    <%= local.document_type %>
  </td>
  <td>
    <%= local.document_number %>
  </td>
  <td>
    <%= l local.date_of_birth %>
  </td>
  <td>
    <%= local.postal_code %>
  </td>
  <td>
    <%= link_to t("admin.actions.edit"),
                edit_admin_census_local_path(local),
                class: "button hollow" %>

    <%= link_to t("admin.actions.delete"),
                admin_census_local_path(local),
                method: :delete,
                class: "button hollow alert",
                data: { confirm: t("admin.actions.confirm") } %>
  </td>
</tr>
